{
  "name": "frost-api",
  "version": "0.0.0",
  "description": "Frost API Server",
  "private": true,
  "main": "./built/index.js",
  "scripts": {
    "start": "node ./",
    "clean": "rimraf ./built",
    "build-copy": "cpx \"src/**/*.js\" built",
    "build-babel": "babel built --out-dir built",
    "build": "npm run clean && npm run build-copy && npm run build-babel && npm run test",
    "test": ""
  },
  "author": "FrostDevelop team",
  "license": "MIT",
  "dependencies": {
    "express": "v4.14.0",
    "body-parser": "^1.15.2",
    "cpx": "v1.3.2",
    "methods": "v1.1.2",
    "mongodb": "v2.2.6",
    "rimraf": "v2.5.4"
  },
  "devDependencies": {
    "babel-cli": "v6.11.4",
    "babel-plugin-transform-async-to-generator": "v6.8.0"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator"
    ]
  }
}
